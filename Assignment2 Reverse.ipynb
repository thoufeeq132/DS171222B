   "source": [
    "def reverse(s):\n",
    "    str = \"\"\n",
    "    for i in s:\n",
    "        str = i + str\n",
    "    return str\n",
    "  \n",
    "s = input()\n",
    "  \n",
    "print(\"original string is : \", end=\"\")\n",
    "print(s)\n",
    "  \n",
    "print(\"reversed string is : \", end=\"\")\n",
    "print(reverse(s))"
   ]
  },
